<%= week_calendar number_of_weeks: 2 do |date| %>
  <%= link_to date ,"/meeting/#{date}/new" %><br>
  
  <% @meetings.each do |meeting| %>

  
  <% if meeting.start_time.year == date.year %>
  <% if meeting.start_time.month == date.month %>
  <% if meeting.start_time.day == date.day %>
  
    <span class="user">
      <%= link_to meeting.user.name, meeting.user %>
    </span>
    <span class="content">
      <%= meeting.title %>
    </span>

  <% end %>
  <% end %>
  <% end %>
  
  <% end %>
  <%= will_paginate @me, :previous_label => ' &lt 前へ', :next_label => '次へ &gt' %>
<% end %>
